def getPositiveNumber(alist): 
    result = []
    for i in alist:
        if i == 0:
            break
        result.append(i)
    return result

alist = list(map(int, input().split()))
alist = getPositiveNumber(alist)

print(len(alist))

for i in alist: 
    if i% 2 == 0: 
        print(i//2, end = ' ') 
    else: 
        print(i*2, end = ' ')
